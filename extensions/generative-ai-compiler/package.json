{
	"name": "generative-ai-compiler",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-bundle"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "endpoint",
				"name": "generative-compiler-endpoint",
				"source": "src/endpoints/generative-endpoints.js"
			},
			{
				"type": "hook",
				"name": "generative-compiler-hooks",
				"source": "src/hooks/generative-hooks.js"
			}
		],
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"add": "directus-extension add"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "12.0.1"
	},
	"dependencies": {
		"axios": "^1.7.4"
	}
}
